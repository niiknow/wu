    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3",
    "component-cookie": "github:component/cookie",
    "component-emitter": "github:component/emitter",
    "debounce": "github:component/debounce",
    "domify": "github:component/domify"
